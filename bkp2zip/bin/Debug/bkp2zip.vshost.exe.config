<?xml version="1.0" encoding="utf-8" ?>
<!--
  SourcePath    Path que irá ser zipado.
  BackupPath    Path onde os arquivos .zip serão armazenados.
  SecurityCopy  Habilita a copia de segurança.
                A copia de segurança irá gerar um arquivo novo caso o executável rode mais de uma vez no mesmo dia.
  Rotation      Habilita a rotação dos arquivos.
  RotationType  Define a periodicidade da rotação.
                1 Semanal
                2 Mensal
                3 Anual
-->
<configuration>
  <appSettings>
    <add key="SourcePath" value=""/>
    <add key="BackupPath" value=""/>
    <add key="SecurityCopy" value="False"/>
    <add key="Rotation" value="False"/>
    <add key="RotationType" value="1"/> 
  </appSettings>
</configuration>